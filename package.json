{
  "name": "@nowarp/misti",
  "version": "0.2.2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nowarp/misti.git"
  },
  "homepage": "https://nowarp.github.io/tools/misti/",
  "description": "TON Static Analyzer",
  "keywords": [
    "static analysis",
    "souffle",
    "ton",
    "blockchain",
    "smart-contracts"
  ],
  "author": "Georgiy Komarov <jubnzv@gmail.com>",
  "license": "Apache-2.0",
  "scripts": {
    "clean": "rm -fr dist docs/*",
    "build": "tsc && cp -R src/detectors/templates dist/src/detectors/",
    "test": "jest",
    "lint": "eslint src test examples",
    "fmt": "prettier --check src test examples",
    "spell": "cspell \"**\" --no-progress --exclude \"dist/**\" --exclude \"node_modules/**\" --exclude \"tags/**\"",
    "docs": "yarn typedoc --out docs/api --entryPointStrategy expand src",
    "test-all": "yarn test && yarn lint && yarn fmt && yarn spell",
    "misti": "ts-node src/main.ts",
    "release": "yarn build && yarn release-it"
  },
  "files": [
    "dist/**/*",
    "src/**/*",
    "bin/**/*"
  ],
  "main": "./dist/src/main.js",
  "bin": {
    "misti": "./bin/misti"
  },
  "dependencies": {
    "@tact-lang/compiler": "^1.4.4",
    "@types/fs-extra": "^11.0.4",
    "@types/js-yaml": "^4.0.9",
    "@types/json-bigint": "^1.0.4",
    "commander": "^12.0.0",
    "fs-extra": "^11.2.0",
    "js-yaml": "^4.1.0",
    "json-bigint": "^1.0.0",
    "semver": "^7.6.3",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "@release-it/keep-a-changelog": "^5.0.0",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.2.3",
    "@types/node": "^20.12.4",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^7.0.4",
    "@typescript-eslint/parser": "^7.0.4",
    "cspell": "^8.12.1",
    "eslint": "^8.57.0",
    "fast-check": "^3.16.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "release-it": "^17.6.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typedoc": "^0.26.5",
    "typescript": "^4.9.3"
  },
  "prettier": {
    "semi": true,
    "printWidth": 80
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "release-it": {
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/keep-a-changelog": {
        "filename": "CHANGELOG.md"
      }
    }
  },
  "packageManager": "yarn@1.22.22"
}
