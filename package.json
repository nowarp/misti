{
  "name": "misti",
  "version": "0.0.1",
  "repository": "https://github.com/nowarp/misti/",
  "homepage": "https://nowarp.github.io/misti/",
  "description": "TON Static Analyser",
  "author": "",
  "license": "Apache-2.0",
  "scripts": {
    "clean": "rm -fr dist",
    "build": "tsc",
    "test": "tsc && jest",
    "lint": "eslint src test examples",
    "fmt": "prettier --check src test examples",
    "docs": "yarn typedoc --out docs/api --entryPointStrategy expand src",
    "misti": "ts-node src/main.ts"
  },
  "files": [
    "dist/**/*",
    "src/**/*",
    "bin/**/*"
  ],
  "main": "./dist/src/main.js",
  "bin": {
    "misti": "./bin/misti"
  },
  "dependencies": {
    "@tact-lang/compiler": "^1.4.0",
    "@types/js-yaml": "^4.0.9",
    "@types/json-bigint": "^1.0.4",
    "commander": "^12.0.0",
    "js-yaml": "^4.1.0",
    "json-bigint": "^1.0.0",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.2.3",
    "@types/node": "^20.12.4",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^7.0.4",
    "@typescript-eslint/parser": "^7.0.4",
    "eslint": "^8.57.0",
    "fast-check": "^3.16.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.13",
    "typescript": "^4.9.3"
  },
  "prettier": {
    "semi": true,
    "printWidth": 80
  }
